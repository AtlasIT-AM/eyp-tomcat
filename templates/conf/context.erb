<?xml version="1.0" encoding="utf-8"?>
<Context <% if defined?(@path) %>path="<%= @path %>"<% end %> <% if defined?(@session_cookie_name) %>sessionCookieName="<%= @session_cookie_name %>"<% end %> <% if defined?(@sessionCookiePath) %>sessionCookiePath="<%= @sessionCookiePath %>"<% end %> antiJARLocking="<%= @antiJARLocking %>" antiResourceLocking="<%= @antiResourceLocking %>">
    <WatchedResource><%= @watchedResource %></WatchedResource>
    <Manager pathname="<%= @manager %>" />
</Context>
